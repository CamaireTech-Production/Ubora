{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "node server/dev-server.js",
    "dev:full": "concurrently \"npm run dev:server\" \"npm run dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server/production-server.js",
    "render-build": "npm run build",
    "deploy:dev": "node scripts/deploy-dev.js",
    "deploy:prod": "node scripts/deploy-prod.js",
    "deploy:dev:local": "npm run deploy:dev && echo 'Development package ready for manual FTP upload'",
    "deploy:prod:local": "npm run deploy:prod && echo 'Production package ready for manual FTP upload'"
  },
  "dependencies": {
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase": "^12.2.1",
    "firebase-admin": "^12.7.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.2",
    "lucide-react": "^0.344.0",
    "openai": "^4.28.0",
    "pdfjs-dist": "^5.4.149",
    "postcss": "^8.4.35",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.2",
    "react-to-print": "^3.1.1",
    "recharts": "^3.2.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
